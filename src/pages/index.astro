---
import About from '@components/About/About.astro';
// import Contact from '@components/Contact/Contact.astro';
// components
import Hero from '@components/Hero/Hero.astro';
import Routes from '@components/Routes/Routes.astro';
import Sponsors from '@components/Sponsors/Sponsors.astro';
import Summary from '@components/Summary/Summary.astro';
import Volunteer from '@components/Volunteer/Volunteer.astro';
// data
import siteData from '@config/siteData.json';
// layout
import BaseLayout from '@layouts/BaseLayout.astro';
